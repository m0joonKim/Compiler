int f(int i, int j) {
  int iarr[4];
  bool barr[4];
  int result;

  result = 0;

  iarr[0] = 10;
  iarr[1] = 20;
  iarr[2] = 30;
  iarr[3] = 40;

  barr[0] = true;
  barr[1] = barr[0];
  barr[2] = false;
  barr[3] = barr[2];

  if (0 <= i && i < 4 && !(iarr[i] > 25)) {
    result = result + 10;
  }

  if (!(0 <= j && j < 3 && (barr[j] || barr[j + 1]))) {
    result = result + 5;
  }

  return result;
}
